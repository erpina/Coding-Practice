#include <bits/stdc++.h>
using namespace std;

int print_left_rotate(int arr[], int n, int left_rotateby)
{
    for (int i=left_rotateby; i<left_rotateby+n; i++ )
    {
        cout << arr[i%n];
    }
}

int main()
{
    int arr[]={1,2,3,4,5};
    int n= sizeof(arr)/sizeof(arr[0]);
    int left_rotateby=3;
    print_left_rotate(arr,n,left_rotateby);

    return 0;
}
